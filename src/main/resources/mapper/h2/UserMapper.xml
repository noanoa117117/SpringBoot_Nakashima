<?xml version ="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//maybatis.org//DTD MAPPER 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!--  MAPPERとxmlのマッピング -->
<mapper namespace="com.example.repository.UserMapper">

<!--  ユーザー1件登録 -->
<insert id="insertOne">
	insert into m_user(
		user_id
		,password
		,user_name
		,birthday
		,age
		,gender
		,department_id
		,role
		)
		values(
		#{userId}
		,#{password}
		,#{userName}
		,#{birthday}
		,#{age}
		,#{gender}
		,#{departmentId}
		,#{role}
		)
</insert>
</mapper>